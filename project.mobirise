0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"text-center\" mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-md-12 col-lg-9\">\n                <div class=\"image-wrap mb-4\" mbr-if=\"showImage\">\n                    <img src=\"@PROJECT_PATH@/assets/images/ahc-full-logo-1646x1646.png\" alt=\"Mobirise\">\n                </div>\n                <h1 class=\"mbr-section-title mbr-fonts-style mb-3\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>Conference Abstracts</b></h1>\n                <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\"></p>\n                <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn mt-3\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-primary\" href=\"AHC_Abstracts.html#content4-38\" data-app-placeholder=\"Type Text\">Session 1 Opening</a>\n                    <a class=\"btn btn-primary\" href=\"AHC_Abstracts.html#content4-39\" data-app-placeholder=\"Type Text\">Session 2 Human Health</a> <a class=\"btn btn-primary\" href=\"AHC_Abstracts.html#content4-3a\" data-app-placeholder=\"Type Text\">Session 3 Agriculture, Aquaculture <br>and Food Production</a> <a class=\"btn btn-primary\" href=\"AHC_Abstracts.html#content4-40\" data-app-placeholder=\"Type Text\">Session 4 Biodiversity &amp; Conservation</a> <a class=\"btn btn-primary\" href=\"AHC_Abstracts.html#content4-45\" data-app-placeholder=\"Type Text\">Session 5&nbsp;Technical &amp; Methodological Perspectives</a>  <a class=\"btn btn-primary\" href=\"assets/files/Poster.pdf\" data-app-placeholder=\"Type Text